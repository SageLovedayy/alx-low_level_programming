# Makefile

# Compiler
CC = gcc

# Source files
SRCS = main.c school.c

# Target executable
TARGET = school

# Default target
all: $(TARGET)

# Build rule for the target executable
$(TARGET): $(SRCS) m.h
	$(CC) $(SRCS) -o $(TARGET)

# Phony target to clean up object files and executable
clean:
	rm -f $(TARGET)
